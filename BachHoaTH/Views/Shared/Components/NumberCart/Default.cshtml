@model List<BachHoaTH.ModelViews.CartItem>
@if (Model != null && Model.Count() > 0)
{
    <li>
        <a href="cart.html">
            <i class="pe-7s-shopbag"></i>
            <span >@Model.Count()</span>
        </a>
    </li>
}
else
{
    <li >
        <a href="cart.html">
            <i class="pe-7s-shopbag"></i>
            <span>0</span>
        </a>
    </li>
}
